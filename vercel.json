{
  "redirects": [
    { "source": "/category/:path*", "destination": "/410", "statusCode": 308 },
    { "source": "/tag/:path*", "destination": "/410", "statusCode": 308 },
    { "source": "/author/:path*", "destination": "/410", "statusCode": 308 },

    { "source": "/feed", "destination": "/410", "statusCode": 308 },
    { "source": "/comments/feed", "destination": "/410", "statusCode": 308 },

    { "source": "/wp-admin/:path*", "destination": "/410", "statusCode": 308 },
    { "source": "/wp-login.php", "destination": "/410", "statusCode": 308 },
    { "source": "/xmlrpc.php", "destination": "/410", "statusCode": 308 },
    { "source": "/wp-content/:path*", "destination": "/410", "statusCode": 308 },
    { "source": "/wp-includes/:path*", "destination": "/410", "statusCode": 308 }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
